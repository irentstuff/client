{
  "name": "irentstuff-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "server": "npx json-server json-mock-api/src/db.json --watch --port 5000",
    "test": "jest --runInBand",
    "start:test": "npm-run-all --continue-on-error dev wait-and-test",
    "wait-and-test": "wait-on http://localhost:5173 && npm run test && npx kill-port 5173"
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@aws-amplify/cli": "^12.12.6",
    "@aws-amplify/ui-react": "^6.2.1",
    "@reduxjs/toolkit": "^2.2.7",
    "antd": "^5.19.4",
    "aws-amplify": "^6.5.4",
    "axios": "^1.7.3",
    "cors": "^2.8.5",
    "jest": "^29.7.0",
    "json-server": "^1.0.0-beta.2",
    "mocha": "10.7.3",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-highlight-words": "^0.20.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.0",
    "selenium-webdriver": "^4.25.0",
    "semgrep": "^0.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
